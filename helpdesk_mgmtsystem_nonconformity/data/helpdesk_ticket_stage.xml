            name="nonconformity_stage_id"
            ref="mgmtsystem_nonconformity.stage_draft"
        />
    </record>
    <record
        id="helpdesk_mgmt.helpdesk_ticket_stage_in_progress"
        model="helpdesk.ticket.stage"
    >
        <field
            name="nonconformity_stage_id"
            ref="mgmtsystem_nonconformity.stage_analysis"
        />
    </record>
    <record
        id="helpdesk_mgmt.helpdesk_ticket_stage_awaiting"
        model="helpdesk.ticket.stage"
    >
        <field
            name="nonconformity_stage_id"
            ref="mgmtsystem_nonconformity.stage_pending"
        />
