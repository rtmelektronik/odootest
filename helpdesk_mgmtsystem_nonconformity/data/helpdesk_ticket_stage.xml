    <record id="helpdesk_mgmt.helpdesk_ticket_stage_done" model="helpdesk.ticket.stage">
        <field
            name="nonconformity_stage_id"
            ref="mgmtsystem_nonconformity.stage_done"
        />
    </record>
    <record
        id="helpdesk_mgmt.helpdesk_ticket_stage_cancelled"
        model="helpdesk.ticket.stage"
    >
        <field
            name="nonconformity_stage_id"
            ref="mgmtsystem_nonconformity.stage_cancel"
        />
    </record>
